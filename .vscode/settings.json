    ":class\\s+\"([^\"]*)\"",
    ":[\\w-.#>]+\\.([\\w-]*)"
  ],
  "tailwindCSS.includeLanguages": {
    "clojure": "html"
  },
  "cSpell.words": [
    "bidi",
    "binaryage",
    "classname",
    "evenodd",
    "htmlfor",
    "Katsuyama",
    "kibu",
    "lambdaisland",
    "malli",
    "postwalk",
    "prewalk",
    "ratom",
    "rippleui",
    "seafoam",
    "tailwindcss",
    "taoensso"
  ]
